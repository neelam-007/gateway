<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="1.0">
	<info organisation="com.l7tech"
		module="AuditSignatureChecker"
		status="integration"
	>
	</info>
	<configurations>
        <include file="../../etc/build/ivyconfigurations.xml"/>
	</configurations>
	<publications>
		<artifact name="AuditSignatureChecker" type="jar" ext="jar" conf="master"/>
	</publications>
	<dependencies>
        <dependency name="layer7-common" rev="latest.integration" force="true" conf="core"/>
        <dependency name="layer7-gui" rev="latest.integration" force="true" conf="core"/>
        <dependency name="layer7-utility" rev="latest.integration" force="true" conf="core"/>
        <dependency name="layer7-gateway-common" rev="latest.integration" force="true" conf="core"/>
        <dependency org="com.rsa" name="jcmFIPS" rev="6.2.0.1" force="true" conf="core"/>
        <dependency org="com.rsa" name="cryptojcommon" rev="6.2.0.1" force="true" conf="core"/>
        <dependency org="com.rsa" name="cryptojce" rev="6.2.0.1" force="true" conf="core"/>
		<dependency org="commons-codec" name="commons-codec" rev="1.6" force="true" conf="core"/>
        <dependency org="commons-collections" name="commons-collections" rev="3.2.2" force="true" conf="core"/>
        <dependency org="commons-logging" name="commons-logging" rev="1.1.1" force="true" conf="core"/>
        <dependency org="com.whirlycott" name="whirlycache" rev="1.0.1" force="true" conf="core"/>
        <dependency org="bouncycastle" name="bcprov-jdk16" rev="1.46" force="true" conf="core"/>

        <!-- This needs the signed version of bouncy castle because it registers it as a provider. -->
        <exclude org="bouncycastle" module="bcprov-unsigned-jdk16" artifact="*" type="*" ext="jar" conf="*" matcher="exact" />
	</dependencies>
</ivy-module>
