<?xml version="1.0" encoding="UTF-8"?>
<wsp:Policy xmlns:L7p="http://www.layer7tech.com/ws/policy" xmlns:wsp="http://schemas.xmlsoap.org/ws/2002/12/policy">
    <wsp:ExactlyOne wsp:Usage="Required">
        <wsp:All wsp:Usage="Required">
            <L7p:TrueAssertion/>
            <wsp:OneOrMore wsp:Usage="Required">
                <L7p:HttpBasic/>
                <L7p:HttpDigest/>
                <L7p:HttpNegotiate/>
                <wsse:SecurityToken xmlns:wsse="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd">
                    <wsse:TokenType>http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd#UsernameToken</wsse:TokenType>
                    <L7p:Properties/>
                </wsse:SecurityToken>
                <L7p:WssDigest/>
                <wsp:All wsp:Usage="Required">
                    <L7p:FalseAssertion/>
                </wsp:All>
                <wsp:ExactlyOne wsp:Usage="Required">
                    <L7p:FalseAssertion/>
                </wsp:ExactlyOne>
                <wsp:OneOrMore wsp:Usage="Required">
                    <L7p:FalseAssertion/>
                </wsp:OneOrMore>
                <L7p:FalseAssertion/>
                <L7p:SslAssertion/>
                <L7p:HttpRoutingAssertion>
                    <L7p:RequestHeaderRules httpPassthroughRuleSet="included">
                        <L7p:Rules httpPassthroughRules="included">
                            <L7p:item httpPassthroughRule="included">
                                <L7p:Name stringValue="Cookie"/>
                            </L7p:item>
                            <L7p:item httpPassthroughRule="included">
                                <L7p:Name stringValue="SOAPAction"/>
                            </L7p:item>
                        </L7p:Rules>
                    </L7p:RequestHeaderRules>
                    <L7p:RequestParamRules httpPassthroughRuleSet="included">
                        <L7p:ForwardAll booleanValue="true"/>
                        <L7p:Rules httpPassthroughRules="included"/>
                    </L7p:RequestParamRules>
                    <L7p:ResponseHeaderRules httpPassthroughRuleSet="included">
                        <L7p:Rules httpPassthroughRules="included">
                            <L7p:item httpPassthroughRule="included">
                                <L7p:Name stringValue="Set-Cookie"/>
                            </L7p:item>
                        </L7p:Rules>
                    </L7p:ResponseHeaderRules>
                </L7p:HttpRoutingAssertion>
                <L7p:BridgeRoutingAssertion>
                    <L7p:RequestHeaderRules httpPassthroughRuleSet="included">
                        <L7p:Rules httpPassthroughRules="included">
                            <L7p:item httpPassthroughRule="included">
                                <L7p:Name stringValue="Cookie"/>
                            </L7p:item>
                            <L7p:item httpPassthroughRule="included">
                                <L7p:Name stringValue="SOAPAction"/>
                            </L7p:item>
                        </L7p:Rules>
                    </L7p:RequestHeaderRules>
                    <L7p:RequestParamRules httpPassthroughRuleSet="included">
                        <L7p:ForwardAll booleanValue="true"/>
                        <L7p:Rules httpPassthroughRules="included"/>
                    </L7p:RequestParamRules>
                    <L7p:ResponseHeaderRules httpPassthroughRuleSet="included">
                        <L7p:Rules httpPassthroughRules="included">
                            <L7p:item httpPassthroughRule="included">
                                <L7p:Name stringValue="Set-Cookie"/>
                            </L7p:item>
                        </L7p:Rules>
                    </L7p:ResponseHeaderRules>
                </L7p:BridgeRoutingAssertion>
                <L7p:JmsRoutingAssertion/>
                <L7p:TrueAssertion/>
                <L7p:MemberOfGroup/>
                <L7p:SpecificUser/>
                <wsse:SecurityToken xmlns:wsse="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd">
                    <wsse:TokenType>http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-x509-token-profile-1.0#X509v3</wsse:TokenType>
                    <L7p:Properties/>
                </wsse:SecurityToken>
                <wsse:SecurityToken xmlns:wsse="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd">
                    <wsse:TokenType>http://schemas.xmlsoap.org/ws/2004/04/security/sc/sct</wsse:TokenType>
                    <L7p:Properties/>
                </wsse:SecurityToken>
                <L7p:RequireWssSignedElement/>
                <L7p:RequireWssEncryptedElement/>
                <L7p:WssSignElement/>
                <L7p:WssEncryptElement/>
                <wsp:MessagePredicate
                    Dialect="http://www.w3.org/TR/1999/REC-xpath-19991116"
                    wsp:Usage="wsp:Required" xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/">/soapenv:Envelope</wsp:MessagePredicate>
                <L7p:ResponseXpathAssertion/>
                <L7p:WssReplayProtection/>
                <L7p:RequestSwAAssertion/>
                <wsse:SecurityToken xmlns:wsse="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd">
                    <wsse:TokenType>urn:oasis:names:tc:SAML:1.0:assertion#Assertion</wsse:TokenType>
                    <L7p:SamlParams>
                        <L7p:NameFormats stringArrayValue="included"/>
                        <L7p:SubjectConfirmations stringArrayValue="included"/>
                    </L7p:SamlParams>
                </wsse:SecurityToken>
                <L7p:RequireWssTimestamp/>
                <L7p:AddWssTimestamp/>
                <L7p:AuditAssertion/>
                <L7p:CustomAssertion>
                    <L7p:base64SerializedValue></L7p:base64SerializedValue>
                </L7p:CustomAssertion>
                <L7p:WsTrustCredentialExchange/>
                <L7p:WsFederationPassiveTokenExchange/>
                <L7p:WsFederationPassiveTokenRequest/>
                <L7p:Regex/>
                <L7p:UnknownAssertion/>
                <L7p:ThroughputQuota/>
                <L7p:EmailAlert/>
                <L7p:CommentAssertion/>
                <L7p:SqlAttackProtection/>
                <L7p:OversizedText/>
                <L7p:RequestSizeLimit/>
                <L7p:Kerberos/>
                <L7p:EncryptedUsernameToken/>
                <L7p:WsiBspAssertion/>
                <L7p:WsspAssertion/>
                <L7p:FaultLevel>
                    <L7p:LevelInfo soapFaultLevel="included"/>
                </L7p:FaultLevel>
                <wsse:SecurityToken xmlns:wsse="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd">
                    <wsse:TokenType>urn:oasis:names:tc:SAML:2.0:assertion#Assertion</wsse:TokenType>
                    <L7p:SamlParams>
                        <L7p:NameFormats stringArrayValue="included"/>
                        <L7p:SubjectConfirmations stringArrayValue="included"/>
                    </L7p:SamlParams>
                </wsse:SecurityToken>
                <L7p:SchemaValidation/>
                <L7p:XslTransformation/>
                <L7p:TimeRange/>
                <L7p:RemoteIpAddressRange/>
                <L7p:AuditDetailAssertion/>
                <L7p:XpathCredentialSource/>
                <L7p:SamlBrowserArtifact>
                    <L7p:AuthenticationProperties authenticationInfo="included"/>
                </L7p:SamlBrowserArtifact>
                <L7p:HttpFormPost>
                    <L7p:FieldInfos fieldInfoArray="included"/>
                </L7p:HttpFormPost>
                <L7p:InverseHttpFormPost>
                    <L7p:FieldNames stringArrayValue="included"/>
                </L7p:InverseHttpFormPost>
                <L7p:WSDLOperation/>
                <L7p:HardcodedResponse/>
                <L7p:AddWssSecurityToken/>
                <L7p:SetVariable/>
                <L7p:CookieCredentialSource/>
                <L7p:HtmlFormDataAssertion>
                    <L7p:FieldSpecs htmlFormFieldSpecArray="included"/>
                </L7p:HtmlFormDataAssertion>
                <L7p:CodeInjectionProtectionAssertion>
                    <L7p:Protections codeInjectionProtectionTypeArray="included"/>
                </L7p:CodeInjectionProtectionAssertion>
                <L7p:Authentication/>
                <L7p:WsiSamlAssertion/>
                <L7p:SamlIssuer/>
                <L7p:Include/>
                <L7p:WssVersion/>
                <L7p:PreemptiveCompression/>
                <L7p:RemoteDomainIdentityInjection/>
                <L7p:WsSecurity/>
                <L7p:RemoveElement/>
            </wsp:OneOrMore>
        </wsp:All>
        <wsp:ExactlyOne wsp:Usage="Required">
            <L7p:TrueAssertion/>
            <L7p:FalseAssertion/>
            <L7p:HttpRoutingAssertion>
                <L7p:Login stringValue="bob&amp;joe"/>
                <L7p:MaxConnections intValue="-5"/>
                <L7p:Password stringValue="james;bloo=foo&amp;goo&quot;poo&quot;\sss\"/>
                <L7p:ProtectedServiceUrl stringValue="http://floomp.boomp.foomp/"/>
                <L7p:Realm stringValue=""/>
                <L7p:RequestHeaderRules httpPassthroughRuleSet="included">
                    <L7p:Rules httpPassthroughRules="included">
                        <L7p:item httpPassthroughRule="included">
                            <L7p:Name stringValue="Cookie"/>
                        </L7p:item>
                        <L7p:item httpPassthroughRule="included">
                            <L7p:Name stringValue="SOAPAction"/>
                        </L7p:item>
                    </L7p:Rules>
                </L7p:RequestHeaderRules>
                <L7p:RequestParamRules httpPassthroughRuleSet="included">
                    <L7p:ForwardAll booleanValue="true"/>
                    <L7p:Rules httpPassthroughRules="included"/>
                </L7p:RequestParamRules>
                <L7p:ResponseHeaderRules httpPassthroughRuleSet="included">
                    <L7p:Rules httpPassthroughRules="included">
                        <L7p:item httpPassthroughRule="included">
                            <L7p:Name stringValue="Set-Cookie"/>
                        </L7p:item>
                    </L7p:Rules>
                </L7p:ResponseHeaderRules>
            </L7p:HttpRoutingAssertion>
            <wsp:MessagePredicate
                Dialect="http://www.w3.org/TR/1999/REC-xpath-19991116"
                wsp:Usage="wsp:Required"
                xmlns:abc="http://namespaces.somewhere.com/abc#bletch" xmlns:blee="http://namespaces.nowhere.com/asdf/fdsa/qwer#blortch.1.2">//blee:blaz</wsp:MessagePredicate>
            <wsp:All wsp:Usage="Required">
                <L7p:RequestSwAAssertion>
                    <L7p:Bindings mapValue="included">
                        <L7p:entry>
                            <L7p:key stringValue="serviceBinding1"/>
                            <L7p:value wsdlBindingInfo="included">
                                <L7p:BindingName stringValue="serviceBinding1"/>
                                <L7p:BindingOperations mapValue="included">
                                    <L7p:entry>
                                    <L7p:key stringValue="getQuote"/>
                                    <L7p:value wsdlBindingOperationInfo="included">
                                    <L7p:Multipart mapValue="included">
                                    <L7p:entry>
                                    <L7p:key stringValue="portfolioData"/>
                                    <L7p:value wsdlMimePartInfo="included">

                                    <L7p:ContentTypes stringArrayValue="included">
                                    <L7p:item stringValue="application/x-zip-compressed"/>
                                    </L7p:ContentTypes>
                                    <L7p:Name stringValue="portfolioData"/>
                                    </L7p:value>
                                    </L7p:entry>
                                    </L7p:Multipart>
                                    <L7p:Name stringValue="getQuote"/>
                                    </L7p:value>
                                    </L7p:entry>
                                </L7p:BindingOperations>
                            </L7p:value>
                        </L7p:entry>
                    </L7p:Bindings>
                </L7p:RequestSwAAssertion>
            </wsp:All>
        </wsp:ExactlyOne>
        <L7p:TrueAssertion/>
        <L7p:FalseAssertion/>
        <L7p:CustomAssertion>
            <L7p:base64SerializedValue></L7p:base64SerializedValue>
        </L7p:CustomAssertion>
    </wsp:ExactlyOne>
</wsp:Policy>
