<mappings>
  <mapping xmlns:tns="http://objectmodel.l7tech.com">

    <!-- Bug in XFire?
         If an AEGIS mapping file exists, namespace must be explicitly specified
         for properties. Otherwise, they will inherit the wrong default
         namespace from the parent element in the SOAP response. -->
    <property name="name" mappedName="tns:name"/>
    <property name="oid" mappedName="tns:oid"/>

    <!-- Properties not published. -->
    <property name="description" ignore="true"/>
    <property name="strId" ignore="true"/>
    <property name="type" ignore="true"/>

  </mapping>
</mappings>
