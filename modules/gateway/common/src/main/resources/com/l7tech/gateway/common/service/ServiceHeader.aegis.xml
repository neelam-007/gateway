<mappings xmlns:ons="http://objectmodel.l7tech.com">
    <mapping name="ons:ServiceHeader">

        <property name="name" mappedName="ons:name"/>
        <property name="oid" mappedName="ons:oid"/>

        <!-- Properties not published. -->
        <property name="description" ignore="true"/>
        <property name="strId" ignore="true"/>
        <property name="type" ignore="true"/>
        <property name="folderOid" ignore="true"/>
        <property name="alias" ignore="true"/>
        <property name="soap" ignore="true"/>
        <property name="disabled" ignore="true"/>
        <property name="displayName" ignore="true"/>
    </mapping>

</mappings>
