<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC  "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

    <!--
    CA Unicenter WSDM observer.

    Note that serverConfig has to initialize first, since it will set the configDirectory
    system property.
    -->
    <bean id="caWsdmRoutingListener"
          class="com.l7tech.server.module.ca_wsdm.CaWsdmObserver"
          depends-on="serverConfig">
    </bean>
</beans>
