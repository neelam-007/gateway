<?xml version="1.0" encoding="UTF-8"?>
<!-- Ant build file for the modular WhichModuleAssertion. -->
<project name="module.assertion.WhichModule" default="package" basedir=".">

    <description>
      Builds the modular WhichModuleAssertion.
    </description>

    <property name="UneasyRooster.dir" location="../../.."/>
    <property name="moduleName" value="WhichModuleAssertion"/>
    <property name="assertionList" value="com.l7tech.skunkworks.WhichModuleAssertion com.l7tech.skunkworks.DumpModulesAssertion com.l7tech.skunkworks.HelloWorldAssertion"/>


    <import file="../assertion-build.xml"/>


    <target name="test" depends="-compile-tests">
        <checkfail/>
    </target>


</project>