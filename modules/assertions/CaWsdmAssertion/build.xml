<?xml version="1.0" encoding="UTF-8"?>
<!-- Ant build file for the modular CaWsdmAssertion. -->
<project name="module.assertion.CaWsdm" default="package" basedir=".">

    <description>
      Builds the modular CaWsdmAssertion.
    </description>

    <property name="UneasyRooster.dir" location="../../.."/>
