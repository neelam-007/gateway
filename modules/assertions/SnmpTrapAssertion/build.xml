<?xml version="1.0" encoding="UTF-8"?>
<!-- Ant build file for the modular SnmpTrapAssertion. -->
<project name="module.assertion.SnmpTrap" default="package" basedir=".">

    <description>
      Builds the modular SnmpTrapAssertion.
    </description>

    <property name="UneasyRooster.dir" location="../../.."/>
    <property name="moduleName" value="SnmpTrapAssertion"/>
    <property name="assertionList" value="com.l7tech.external.assertions.snmptrap.${moduleName}"/>
    <property name="build.classes" location="build/classes"/>


    <import file="../assertion-build.xml"/>


    <target name="test" depends="-compile-tests">
        <checkfail/>
    </target>


</project>