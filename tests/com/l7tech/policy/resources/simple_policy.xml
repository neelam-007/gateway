<?xml version="1.0" encoding="UTF-8"?>
<wsp:Policy xmlns="http://www.layer7tech.com/ws/policy" xmlns:wsp="http://schemas.xmlsoap.org/ws/2002/12/policy">
    <wsp:ExactlyOne wsp:Usage="Required">
        <wsp:All wsp:Usage="Required">
            <TrueAssertion/>
            <wsp:OneOrMore wsp:Usage="Required">
                <HttpBasic/>
                <HttpDigest/>
                <WssBasic/>
                <wsp:All wsp:Usage="Required">
                    <FalseAssertion/>
                </wsp:All>
                <wsp:ExactlyOne wsp:Usage="Required">
                    <FalseAssertion/>
                </wsp:ExactlyOne>
                <wsp:OneOrMore wsp:Usage="Required">
                    <FalseAssertion/>
                </wsp:OneOrMore>
                <FalseAssertion/>
                <SslAssertion/>
                <HttpRoutingAssertion/>
                <BridgeRoutingAssertion/>
                <JmsRoutingAssertion/>
                <TrueAssertion/>
                <MemberOfGroup/>
                <SpecificUser/>
                <wsse:SecurityToken xmlns:wsse="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd">
                    <wsse:TokenType>http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd#X509v3</wsse:TokenType>
                </wsse:SecurityToken>
                <SecureConversation/>
                <RequestWssIntegrity/>
                <RequestWssConfidentiality/>
                <ResponseWssIntegrity/>
                <ResponseWssConfidentiality/>
                <wsp:MessagePredicate
                    Dialect="http://www.w3.org/TR/1999/REC-xpath-19991116"
                    wsp:Usage="wsp:Required" xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/">/soapenv:Envelope</wsp:MessagePredicate>
                <ResponseXpathAssertion/>
                <RequestWssReplayProtection/>
                <RequestSwAAssertion/>
                <RequestWssSaml>
                    <SubjectConfirmations stringArrayValue="included"/>
                    <NameFormats stringArrayValue="included"/>
                </RequestWssSaml>
                <AuditAssertion/>
                <CustomAssertion>
                    <base64SerializedValue></base64SerializedValue>
                </CustomAssertion>
                <WsTrustCredentialExchange/>
                <Regex/>
                <UnknownAssertion/>
                <SnmpTrap/>
                <ThroughputQuota/>
                <EmailAlert/>
            </wsp:OneOrMore>
        </wsp:All>
        <wsp:ExactlyOne wsp:Usage="Required">
            <TrueAssertion/>
            <FalseAssertion/>
            <HttpRoutingAssertion>
                <ProtectedServiceUrl stringValue="http://floomp.boomp.foomp/"/>
                <Login stringValue="bob&amp;joe"/>
                <Password stringValue="james;bloo=foo&amp;goo&quot;poo&quot;\sss\"/>
                <Realm stringValue=""/>
                <MaxConnections intValue="-5"/>
            </HttpRoutingAssertion>
            <wsp:MessagePredicate
                Dialect="http://www.w3.org/TR/1999/REC-xpath-19991116"
                wsp:Usage="wsp:Required"
                xmlns:abc="http://namespaces.somewhere.com/abc#bletch" xmlns:blee="http://namespaces.nowhere.com/asdf/fdsa/qwer#blortch.1.2">//blee:blaz</wsp:MessagePredicate>
            <wsp:All wsp:Usage="Required">
                <RequestSwAAssertion>
                    <Bindings mapValue="included">
                        <entry>
                            <key stringValue="serviceBinding1"/>
                            <value wsdlBindingInfo="included">
                                <BindingName stringValue="serviceBinding1"/>
                                <BindingOperations mapValue="included">
                                    <entry>
                                    <key stringValue="getQuote"/>
                                    <value wsdlBindingOperationInfo="included">
                                    <Multipart mapValue="included">
                                    <entry>
                                    <key stringValue="portfolioData"/>
                                    <value wsdlMimePartInfo="included">
                                    <ContentTypes stringArrayValue="included">
                                    <item stringValue="application/x-zip-compressed"/>
                                    </ContentTypes>
                                    <Name stringValue="portfolioData"/>
                                    </value>
                                    </entry>
                                    </Multipart>
                                    <Name stringValue="getQuote"/>
                                    </value>
                                    </entry>
                                </BindingOperations>
                            </value>
                        </entry>
                    </Bindings>
                </RequestSwAAssertion>
            </wsp:All>
        </wsp:ExactlyOne>
        <TrueAssertion/>
        <FalseAssertion/>
        <CustomAssertion>
            <base64SerializedValue></base64SerializedValue>
        </CustomAssertion>
    </wsp:ExactlyOne>
</wsp:Policy>
