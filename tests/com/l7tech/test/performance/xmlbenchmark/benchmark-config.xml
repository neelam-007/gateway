<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<benchmark-config xmlns="http://l7tech.com/xmlbench"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://l7tech.com/xmlbench benchmark-config.xsd">
    <benchmark-test name="SimpleTest">
        <xmlMessage>
            <location>/com/l7tech/test/performance/xmlbenchmark/benchmark-config.xml</location>
        </xmlMessage>
        <schemaLocation>/com/l7tech/test/performance/xmlbenchmark/benchmark-config.xsd.xml</schemaLocation>
        <xsltLocation>/com/l7tech/test/performance/xmlbenchmark/benchmark-config.xsl.xml</xsltLocation>
        <xpathQueries>
            <xmlnsDef>
                <prefix>ns1</prefix>
                <value>http://l7tech.com/xmlbench</value>
            </xmlnsDef>
            <query>
                <xpath>//ns1:benchmark-test/@name</xpath>
                <value>SimpleTest</value>
            </query>
        </xpathQueries>
    </benchmark-test>

    <benchmark-test name="HL7v3Test">
        <xmlMessage>
            <location>/home/vchan/Dev/workspace/vc-test/cr-schema/Sample-HL7-QUPA_IN101102CA.xml</location>
        </xmlMessage>
        <schemaLocation>/home/vchan/Dev/workspace/vc-test/cr-schema/Compositeschemas/QUPA_IN101102CA.xsd</schemaLocation>
        <xsltLocation>/com/l7tech/test/performance/xmlbenchmark/benchmark-config.xsl.xml</xsltLocation>
        <xpathQueries>
            <xmlnsDef>
                <prefix>ns1</prefix>
                <value>urn:hl7-org:v3</value>
            </xmlnsDef>
            <query>
                <xpath>/ns1:QUPA_IN101102CA/@ITSVersion</xpath>
                <value>XML_1.0</value>
            </query>
        </xpathQueries>
    </benchmark-test>

</benchmark-config>
