<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from IdentityService WSDL service -->

  <service name="identities" provider="java:RPC" style="rpc" use="encoded">
      <parameter name="wsdlTargetNamespace" value="http://www.layer7-tech.com/adminservice/identity"/>
      <parameter name="wsdlServiceElement" value="IdentityService"/>
      <parameter name="wsdlServicePort" value="identities"/>
      <parameter name="className" value="com.l7tech.adminws.identity.IdentitiesSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Identity"/>
      <operation name="findAlllIdentityProviderConfig" qname="operNS:findAlllIdentityProviderConfig" xmlns:operNS="http://www.layer7-tech.com/adminservice/identity" returnQName="findAlllIdentityProviderConfigReturn" returnType="rtns:ArrayOf_tns1_Header" xmlns:rtns="http://www.layer7-tech.com/adminservice/identity" >
      </operation>
      <operation name="findAllIdentityProviderConfigByOffset" qname="operNS:findAllIdentityProviderConfigByOffset" xmlns:operNS="http://www.layer7-tech.com/adminservice/identity" returnQName="findAllIdentityProviderConfigByOffsetReturn" returnType="rtns:ArrayOf_tns1_Header" xmlns:rtns="http://www.layer7-tech.com/adminservice/identity" >
        <parameter name="offset" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter name="windowSize" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="findIdentityProviderConfigByPrimaryKey" qname="operNS:findIdentityProviderConfigByPrimaryKey" xmlns:operNS="http://www.layer7-tech.com/adminservice/identity" returnQName="findIdentityProviderConfigByPrimaryKeyReturn" returnType="rtns:IdentityProviderConfig" xmlns:rtns="http://service.adminws.l7tech.com" >
        <parameter name="oid" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="saveIdentityProviderConfig" qname="operNS:saveIdentityProviderConfig" xmlns:operNS="http://www.layer7-tech.com/adminservice/identity" returnQName="saveIdentityProviderConfigReturn" returnType="rtns:long" xmlns:rtns="http://www.w3.org/2001/XMLSchema" >
        <parameter name="identityProviderConfig" type="tns:IdentityProviderConfig" xmlns:tns="http://service.adminws.l7tech.com"/>
      </operation>
      <operation name="deleteIdentityProviderConfig" qname="operNS:deleteIdentityProviderConfig" xmlns:operNS="http://www.layer7-tech.com/adminservice/identity" >
        <parameter name="oid" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="findUserByPrimaryKey" qname="operNS:findUserByPrimaryKey" xmlns:operNS="http://www.layer7-tech.com/adminservice/identity" returnQName="findUserByPrimaryKeyReturn" returnType="rtns:User" xmlns:rtns="http://service.adminws.l7tech.com" >
        <parameter name="identityProviderConfigId" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter name="userId" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="deleteUser" qname="operNS:deleteUser" xmlns:operNS="http://www.layer7-tech.com/adminservice/identity" >
        <parameter name="identityProviderConfigId" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter name="userId" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="saveUser" qname="operNS:saveUser" xmlns:operNS="http://www.layer7-tech.com/adminservice/identity" returnQName="saveUserReturn" returnType="rtns:long" xmlns:rtns="http://www.w3.org/2001/XMLSchema" >
        <parameter name="identityProviderConfigId" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter name="user" type="tns:User" xmlns:tns="http://service.adminws.l7tech.com"/>
      </operation>
      <operation name="findAllUsers" qname="operNS:findAllUsers" xmlns:operNS="http://www.layer7-tech.com/adminservice/identity" returnQName="findAllUsersReturn" returnType="rtns:ArrayOf_tns1_Header" xmlns:rtns="http://www.layer7-tech.com/adminservice/identity" >
        <parameter name="identityProviderConfigId" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="findAllUsersByOffset" qname="operNS:findAllUsersByOffset" xmlns:operNS="http://www.layer7-tech.com/adminservice/identity" returnQName="findAllUsersByOffsetReturn" returnType="rtns:ArrayOf_tns1_Header" xmlns:rtns="http://www.layer7-tech.com/adminservice/identity" >
        <parameter name="identityProviderConfigId" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter name="offset" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter name="windowSize" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="findGroupByPrimaryKey" qname="operNS:findGroupByPrimaryKey" xmlns:operNS="http://www.layer7-tech.com/adminservice/identity" returnQName="findGroupByPrimaryKeyReturn" returnType="rtns:Group" xmlns:rtns="http://service.adminws.l7tech.com" >
        <parameter name="identityProviderConfigId" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter name="groupId" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="deleteGroup" qname="operNS:deleteGroup" xmlns:operNS="http://www.layer7-tech.com/adminservice/identity" >
        <parameter name="identityProviderConfigId" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter name="groupId" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="saveGroup" qname="operNS:saveGroup" xmlns:operNS="http://www.layer7-tech.com/adminservice/identity" returnQName="saveGroupReturn" returnType="rtns:long" xmlns:rtns="http://www.w3.org/2001/XMLSchema" >
        <parameter name="identityProviderConfigId" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter name="group" type="tns:Group" xmlns:tns="http://service.adminws.l7tech.com"/>
      </operation>
      <operation name="findAllGroups" qname="operNS:findAllGroups" xmlns:operNS="http://www.layer7-tech.com/adminservice/identity" returnQName="findAllGroupsReturn" returnType="rtns:ArrayOf_tns1_Header" xmlns:rtns="http://www.layer7-tech.com/adminservice/identity" >
        <parameter name="identityProviderConfigId" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="findAllGroupsByOffset" qname="operNS:findAllGroupsByOffset" xmlns:operNS="http://www.layer7-tech.com/adminservice/identity" returnQName="findAllGroupsByOffsetReturn" returnType="rtns:ArrayOf_tns1_Header" xmlns:rtns="http://www.layer7-tech.com/adminservice/identity" >
        <parameter name="identityProviderConfigId" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter name="offset" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter name="windowSize" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="saveGroup findAllIdentityProviderConfigByOffset deleteIdentityProviderConfig findAllUsersByOffset findUserByPrimaryKey deleteGroup findAlllIdentityProviderConfig saveUser deleteUser saveIdentityProviderConfig findIdentityProviderConfigByPrimaryKey findAllGroups findAllUsers findGroupByPrimaryKey findAllGroupsByOffset"/>
      <parameter name="scope" value="Application"/>

      <typeMapping
        xmlns:ns="http://www.layer7-tech.com/adminservice/identity"
        qname="ns:ArrayOf_tns1_Header"
        type="java:com.l7tech.adminws.identity.Header[]"
        serializer="org.apache.axis.encoding.ser.ArraySerializerFactory"
        deserializer="org.apache.axis.encoding.ser.ArrayDeserializerFactory"
        encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
      />
      <typeMapping
        xmlns:ns="http://service.adminws.l7tech.com"
        qname="ns:IdentityProviderConfig"
        type="java:com.l7tech.adminws.identity.IdentityProviderConfig"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
      />
      <typeMapping
        xmlns:ns="http://service.adminws.l7tech.com"
        qname="ns:Header"
        type="java:com.l7tech.adminws.identity.Header"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
      />
      <typeMapping
        xmlns:ns="http://service.adminws.l7tech.com"
        qname="ns:Group"
        type="java:com.l7tech.adminws.identity.Group"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
      />
      <typeMapping
        xmlns:ns="http://service.adminws.l7tech.com"
        qname="ns:User"
        type="java:com.l7tech.adminws.identity.User"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
      />
  </service>
</deployment>
